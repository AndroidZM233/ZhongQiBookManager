<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"

    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#4F94CD"

    >
<LinearLayout
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <LinearLayout
        android:background="#FFFFFF"
        android:gravity="center"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="50dp">
        <TextView
            android:layout_gravity="bottom"
            android:background="#FFFFFF"
            android:textSize="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:text="欢迎进入货位调整["
            android:layout_width="wrap_content"
            android:layout_height="35dp" />
        <TextView
            android:layout_gravity="bottom"
            android:id="@+id/goodstrim_department_tv"
            android:background="#FFFFFF"
            android:textSize="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:text="部门名称"
            android:layout_width="wrap_content"
            android:layout_height="35dp" />
        <TextView
            android:layout_gravity="bottom"
            android:background="#FFFFFF"
            android:textSize="20dp"
            android:textColor="#000000"
            android:gravity="center"
            android:text="]"
            android:layout_width="wrap_content"
            android:layout_height="35dp" />

    </LinearLayout>

    <LinearLayout

        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:gravity="center"
            android:textSize="20dp"
            android:layout_marginLeft="7dp"
            android:layout_marginRight="7dp"
            android:text="目标货位"
            android:layout_width="wrap_content"
            android:layout_height="fill_parent" />

        <EditText
            android:layout_margin="5dp"

            android:id="@+id/goodstrim_inputlocation_et"
            android:background="@drawable/shape"
            android:hint=" 请输入目标货位号"

            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="30dp"
            android:text="" />

        <Button
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:id="@+id/goodstrim_searchlocation_btn"
            android:text="搜索"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="30dp" />

    </LinearLayout>

    <LinearLayout

        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <EditText
            android:layout_margin="5dp"

            android:id="@+id/goodstrim_input_et"
            android:background="@drawable/shape"
            android:hint="            输入商品条形码"

            android:layout_width="0dp"
            android:layout_weight="4"
            android:layout_height="30dp"
            android:text="" />

        <Button
            android:layout_margin="5dp"
            android:background="@drawable/shape"
            android:id="@+id/goodstrim_search_btn"
            android:text="搜索"
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="30dp" />

    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:layout_marginLeft="5dp"
            android:text="期末库存:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView

            android:id="@+id/goodstrim_QMKC_tv"
            android:text=""
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />


        <TextView
            android:text="当前货位:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/goodstrim_HWBH_tv"
            android:text=""
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />


        <TextView
            android:text="定价:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/goodstrim_DJ_tv"
            android:text=""
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />



    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">


        <TextView
            android:layout_marginLeft="5dp"
            android:text="当前发行分类:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView

            android:id="@+id/goodstrim_FXFLMC_tv"
            android:text=""
            android:layout_width="0dp"
            android:layout_weight="2.6"
            android:layout_height="wrap_content" />


        <TextView
            android:text="版别:"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/goodstrim_BB_tv"
            android:text=""
            android:layout_width="0dp"
            android:layout_weight="1"
            android:layout_height="wrap_content" />





    </LinearLayout>

    <LinearLayout
        android:layout_marginTop="6dp"
        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">



    <TextView
        android:layout_marginLeft="5dp"
        android:text="品名:"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/goodstrim_PM_tv"
        android:text=""
        android:layout_width="match_parent"

        android:layout_height="wrap_content" />

</LinearLayout>

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginRight="20dp"
        android:layout_marginTop="20dp"
        android:orientation="horizontal">
        <Button
            android:id="@+id/goodstrim_luru_btn"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="录入"
            android:layout_weight="1"
            android:layout_marginRight="30dp"
            android:background="@drawable/bg_alibuybutton"/>

        <Button
            android:id="@+id/goodstrim_clean_btn"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:layout_marginLeft="30dp"
            android:text="清除"
            android:background="@drawable/bg_alibuybutton"/>
    </LinearLayout>




    <LinearLayout

        android:orientation="horizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <Button

            android:id="@+id/goodstrim_browse_btn"
            android:text="浏览"
            android:background="#FFFFFF"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_gravity="bottom"
            android:layout_height="wrap_content" />



        <Button
            android:id="@+id/goodstrim_homepage_btn"
            android:text="主页"
            android:background="#FFFFFF"
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_gravity="bottom"
            android:layout_height="wrap_content" />

    </LinearLayout>



</LinearLayout>
    </LinearLayout>