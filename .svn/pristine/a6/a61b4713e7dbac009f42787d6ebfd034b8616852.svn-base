<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
              android:layout_width="match_parent"
              android:layout_height="match_parent"
              android:layout_above="@+id/left_top"
              android:layout_alignParentLeft="true"
              android:layout_alignParentStart="true"
              android:orientation="vertical">

    <com.speedata.zhongqi.view.CustomTitlebar
        android:id="@+id/titlebar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>

    <CheckBox
        android:id="@+id/checkbox_all"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:text="全选/不选"/>
    <LinearLayout
        android:id="@+id/checkbox_con"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginTop="10dp"
        android:layout_marginLeft="50dp">
        <CheckBox
            android:id="@+id/checkbox_mudidi"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="目的地资料下载"/>
        <CheckBox
            android:id="@+id/checkboxSide"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="网点资料下载"/>
        <CheckBox
            android:id="@+id/checkboxProblem"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="问题原因资料下载"
            android:checked="false"/>
        <CheckBox
            android:id="@+id/checkboxLiucang"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="留仓原因资料下载"/>
        <CheckBox
            android:id="@+id/checkboxYunshuType"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="运输方式资料下载"/>
        <CheckBox
            android:id="@+id/checkboxQianfei"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="欠费资料下载"/>
        <CheckBox
            android:id="@+id/checkboxProductLimit"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="产品时效资料下载"/>
        <CheckBox
            android:id="@+id/checkboxCity"
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="城市资料下载"/>

    </LinearLayout>

    <Button
        android:id="@+id/btnDown"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="5dp"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="5dp"
        android:layout_marginTop="5dp"
        android:background="@drawable/selector_btn"
        android:text="下载"/>
</LinearLayout>